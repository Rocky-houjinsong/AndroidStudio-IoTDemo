<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shj.iotplus_00">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IoTPlus_00">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- LoginActivity注册 -->
        <activity android:name=".login.LoginActivity" android:launchMode="singleTask"/>
        <!-- LoginActivity注册 -->
        <activity android:name=".login.LoginSuccessActivity"/>
        <!-- IntentService注册，并创建新进程运行服务 -->
        <service android:name=".component.MyIntentService" android:process="com.shj.intentservice"/>
    </application>

</manifest>